<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content"
	android:layout_alignParentRight="true"
	android:id="@+id/lly_ppw_add_doc"
	android:layout_height="wrap_content"
	android:background="@drawable/shape_doc_list_upload_ppw_bg"
	android:orientation="vertical">

	<ScrollView
		android:layout_width="match_parent"
		android:layout_height="match_parent">

		<LinearLayout
			android:layout_width="wrap_content"
			android:orientation="vertical"
			android:layout_height="wrap_content">

			<LinearLayout
				android:id="@+id/team_document"
				android:layout_width="match_parent"
				android:paddingLeft="@dimen/dp_19"
				android:paddingRight="@dimen/dp_19"
				android:paddingTop="@dimen/dp_8"
				android:paddingBottom="@dimen/dp_8"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:orientation="horizontal">

				<ImageView
					android:layout_width="@dimen/dp_12"
					android:layout_height="@dimen/dp_15"
					android:layout_gravity="center_vertical"
					android:src="@drawable/icon_pop_file" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerVertical="true"
					android:layout_marginLeft="@dimen/dp_11"
					android:text="@string/mtDocuments"
					android:textColor="@color/white"
					android:textSize="@dimen/sp_13" />
			</LinearLayout>

			<TextView
				android:layout_width="match_parent"
				android:layout_height="0.5dp"
				android:id="@+id/tv_ppw_add_doc_divider1"
				android:background="@color/colorDivider242429" />

			<LinearLayout
				android:id="@+id/take_photo"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:paddingLeft="@dimen/dp_19"
				android:paddingRight="@dimen/dp_19"
				android:paddingTop="@dimen/dp_8"
				android:paddingBottom="@dimen/dp_8"
				android:orientation="horizontal">

				<ImageView
					android:layout_width="@dimen/dp_12"
					android:layout_height="@dimen/dp_15"
					android:layout_gravity="center_vertical"
					android:src="@drawable/icon_pop_take" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerVertical="true"
					android:layout_marginLeft="@dimen/dp_11"
					android:text="@string/mtTPhoto"
					android:textColor="@color/white"
					android:textSize="@dimen/sp_13" />


			</LinearLayout>

			<TextView
				android:layout_width="match_parent"
				android:layout_height="0.5dp"
				android:id="@+id/tv_ppw_add_doc_divider2"
				android:background="@color/colorDivider242429" />

			<LinearLayout
				android:id="@+id/file_library"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:paddingLeft="@dimen/dp_19"
				android:paddingRight="@dimen/dp_19"
				android:paddingTop="@dimen/dp_8"
				android:paddingBottom="@dimen/dp_8"
				android:orientation="horizontal">

				<ImageView
					android:layout_width="@dimen/dp_12"
					android:layout_height="@dimen/dp_15"
					android:layout_gravity="center_vertical"
					android:src="@drawable/icon_pop_photo" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerVertical="true"
					android:layout_marginLeft="@dimen/dp_11"
					android:text="@string/mtLibrary"
					android:textColor="@color/white"
					android:textSize="@dimen/sp_13" />


			</LinearLayout>

			<TextView
				android:layout_width="match_parent"
				android:layout_height="0.5dp"
				android:id="@+id/tv_ppw_add_doc_divider3"
				android:background="@color/colorDivider242429" />

			<LinearLayout
				android:id="@+id/fileststem_library"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:paddingLeft="@dimen/dp_19"
				android:paddingRight="@dimen/dp_19"
				android:paddingTop="@dimen/dp_8"
				android:paddingBottom="@dimen/dp_8"
				android:orientation="horizontal">

				<ImageView
					android:layout_width="@dimen/dp_12"
					android:layout_height="@dimen/dp_15"
					android:layout_gravity="center_vertical"
					android:src="@drawable/icon_file_save" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerVertical="true"
					android:layout_marginLeft="@dimen/dp_11"
					android:text="@string/mtFileLibrary"
					android:textColor="@color/white"
					android:textSize="@dimen/sp_13" />
			</LinearLayout>

			<TextView
				android:layout_width="match_parent"
				android:layout_height="0.5dp"
				android:id="@+id/tv_ppw_add_doc_divider4"
				android:background="@color/colorDivider242429" />

			<LinearLayout
				android:id="@+id/save_file"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:paddingLeft="@dimen/dp_19"
				android:paddingRight="@dimen/dp_19"
				android:paddingTop="@dimen/dp_8"
				android:paddingBottom="@dimen/dp_8"
				android:orientation="horizontal">

				<ImageView
					android:layout_width="@dimen/dp_12"
					android:layout_height="@dimen/dp_15"
					android:layout_gravity="center_vertical"
					android:src="@drawable/icon_pop_favorite" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerVertical="true"
					android:layout_marginLeft="@dimen/dp_11"
					android:text="@string/mtSavedFiles"
					android:textColor="@color/white"
					android:textSize="@dimen/sp_13" />

			</LinearLayout>

			<TextView
				android:layout_width="match_parent"
				android:layout_height="0.5dp"
				android:id="@+id/tv_ppw_add_doc_divider5"
				android:background="@color/colorDivider242429" />

			<LinearLayout
				android:id="@+id/blank_file"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:paddingLeft="@dimen/dp_19"
				android:paddingRight="@dimen/dp_19"
				android:paddingTop="@dimen/dp_8"
				android:paddingBottom="@dimen/dp_8"
				android:orientation="horizontal">

				<ImageView
					android:layout_width="@dimen/dp_12"
					android:layout_height="@dimen/dp_15"
					android:layout_centerVertical="true"
					android:layout_gravity="center_vertical"
					android:src="@drawable/icon_file_save" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerVertical="true"
					android:layout_marginLeft="@dimen/dp_11"
					android:text="@string/cbPage"
					android:textColor="@color/white"
					android:textSize="@dimen/sp_13" />

			</LinearLayout>

		</LinearLayout>
	</ScrollView>
</LinearLayout>