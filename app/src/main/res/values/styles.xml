<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="ThemeActivity" mce_bogus="1" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor" tools:ignore="NewApi">@color/white</item>
        <item name="android:windowAnimationStyle">@style/AnimationActivity</item>
        <item name="android:windowNoTitle">true</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/black</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <!--<item name="android:windowIsTranslucent">true</item>-->
    </style>

    <style name="ThemeActivity3" mce_bogus="1" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor" tools:ignore="NewApi">@color/nocolor</item>
        <item name="android:windowAnimationStyle">@style/AnimationActivity2</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
    </style>

    <style name="ThemeActivity2" mce_bogus="1" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:statusBarColor" tools:ignore="NewApi">@color/nocolor</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="actionBarSize">0dp</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <!--<item name="android:windowIsFloating">true</item>&lt;!&ndash; 是否悬浮在activity上 &ndash;&gt;
        <item name="android:windowContentOverlay">@null</item>&lt;!&ndash; 窗口是否有遮盖 &ndash;&gt;-->
    </style>

    <style name="ThemeActivity4" mce_bogus="1" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:statusBarColor" tools:ignore="NewApi">@color/nocolor</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/nocolor</item>
        <item name="actionBarSize">0dp</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <!--<item name="android:windowIsFloating">true</item>&lt;!&ndash; 是否悬浮在activity上 &ndash;&gt;
        <item name="android:windowContentOverlay">@null</item>&lt;!&ndash; 窗口是否有遮盖 &ndash;&gt;-->
    </style>

